{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "suppin",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#215BFB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suppin.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.suppin.app",
      "versionCode": 1
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e9114904-e18c-4934-975c-3488f0d497ed"
      }
    }
  }
}
